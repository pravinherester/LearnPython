
def turn_right():
    turn_left()
    turn_left()
    turn_left()
def move_3_steps():
    move()
    move()
    move()
    
def move_to_lshape():
    move_3_steps()
    turn_left()
    move_3_steps()
    turn_right()

    
    

move_to_lshape()

move()
turn_right()

move_to_lshape()

move()
turn_right()

move_to_lshape()

move()
turn_right()

move_to_lshape()







    

    
