def three_steps():
    move()
    move()
    move()
def turn_around():
    turn_left()
    turn_left()
three_steps()
turn_around()
three_steps()

def turn_right():
    turn_left()
    turn_left()
    turn_left()
def jump():
    move()
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()

for n in range(0,4):
    jump()

